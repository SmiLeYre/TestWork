{
    "[typescript]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint",
    },
    "editor.codeActionsOnSave": {
      "source.fixAll": "explicit"
    },
    "eslint.validate": [
      "javascript",
      "typescript",
    ],
    "stylelint.validate": [
      "css",
      "less",
      "postcss",
      "scss",
      "vue",
      "sass",
    ],
    "liveServer.settings.port": 5501
  }